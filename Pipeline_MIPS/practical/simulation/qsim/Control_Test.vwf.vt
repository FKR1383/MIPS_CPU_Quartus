`timescale 1 ps/ 1 ps
module Control_vlg_sample_tst(
	ENABLE,
	Funct,
	OP_CODE,
	sampler_tx
);
input  ENABLE;
input [2:0] Funct;
input [3:0] OP_CODE;
output sampler_tx;

reg sample;
time current_time;
always @(ENABLE or Funct or OP_CODE)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

